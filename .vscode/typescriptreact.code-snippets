      "",
      "async function update${1:ApiName}(body: ${1/(.*)/${1:/downcase}/}Body, options: { email: string }) {",
      "  const { email } = options;",
      "  const result = await prisma.${2:table}.update({",
      "    where: {",
      "      id: body.id,",
