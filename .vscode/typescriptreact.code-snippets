      "  const json = await request.json();",
      "  const body = ${1/(.*)/${1:/downcase}/}Body.parse(json);",
      "",
      "  const result = await update${1:ApiName}(body, { email: session.user.email });",
      "",
      "  return NextResponse.json(result);",
      "});",
