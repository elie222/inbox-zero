  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=16384 next dev --turbopack",
    "dev:e2e": "dotenv -e .env.e2e -- cross-env NODE_OPTIONS=--max_old_space_size=16384 next dev --turbopack",
